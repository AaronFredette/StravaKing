@using SK.Library.Classes.Session
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/thirdparty/ratchet-2.0.2/dist/css/ratchet.min.css" rel="stylesheet" />
    <script src="~/Content/thirdparty/ratchet-2.0.2/dist/js/ratchet.min.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Content/thirdparty/angular/angular.min.js"></script>
    <script src="~/Scripts/StravaScripts/CreateChallenge.js"></script>
</head>
<body>
   
                <header class="bar bar-nav">
                    <h1 class="title">@ViewBag.Title</h1>
                @if (SessionManager.GetCurrentUser() != null)
                {
                    @Html.ActionLink("Log Out", "Logout", "Authentication", new {area = ""}, new {@class = "pull-right"})
                }
                </header>


<div class="content">

    @RenderBody()
</div>
    <div>
        @if (SessionManager.GetCurrentUser() != null)
        {
            @Html.Action("Footer", "Global")
        }
    </div>




    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>