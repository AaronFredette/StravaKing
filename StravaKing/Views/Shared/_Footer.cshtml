@using SK.Library.Classes.Session
@model SK.Library.Classes.Models.Users.UserViewModel
@{
    ViewBag.Title = "footer";
}
<div>
    <nav class="bar bar-tab">
        <a class="tab-item active" href="/">
            <span class="icon icon-home"></span>
            <span class="tab-label">Home</span>
        </a>
        <a class="tab-item" href="#">
            <span class="icon icon-person"></span>
            <span class="tab-label">Profile</span>
        </a>
        <a class="tab-item" href="/Challenge/CreateChallenge">
            <span class="icon icon-compose"></span>
            <span class="tab-label">New</span>
        </a>
        <a class="tab-item" href="#">
            <span class="icon icon-list"></span>
            <span class="tab-label">Active</span>
        </a>
        <a class="tab-item" href="#">
            <span class="icon icon-star-filled"></span>
            <span class="tab-label">Completed</span>
        </a>
        @if (SessionManager.GetCurrentUser() != null)
        {
            <a class="tab-item" href="/Authentication/Logout">Log Out</a>
        }

    </nav>
</div>
